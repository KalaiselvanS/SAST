<DataflowCleanseRule formatVersion="%FORMATVERSION%" language="%Language%">
    <RuleID>%RULEID%</RuleID>
    <TaintFlags>+VALIDATED_HEADER_MANIPULATION,+VALIDATED_HEADER_MANIPULATION_COOKIES</TaintFlags>
    <FunctionIdentifier>
        <NamespaceName>
            <Pattern>%FunctionIdentifier.NamespaceName%</Pattern>
        </NamespaceName>
        <ClassName>
            <Pattern>%FunctionIdentifier.ClassName%</Pattern>
        </ClassName>
        <FunctionName>
            <Pattern>%FunctionIdentifier.FunctionName%</Pattern>
        </FunctionName>
        <ApplyTo implements="true" overrides="true" extends="true"/>
    </FunctionIdentifier>
    <OutArguments>%OutArguments%</OutArguments>
</DataflowCleanseRule>
